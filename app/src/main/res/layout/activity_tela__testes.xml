<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/color_background"
    tools:context=".Activity_telaTestes.tela_Testes">


    <RelativeLayout
        android:id="@+id/fragment_Container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="70dp"
        >

    </RelativeLayout>

    <!--View
        android:id="@+id/view_divider02"
        android:layout_marginBottom="4dp"
        android:layout_above="@+id/text_Resultados"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/silver"/-->

    <TextView
        android:id="@+id/text_Resultados"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_Resultados"
        android:layout_above="@+id/field_Resultados"
        android:paddingRight="120dp"
        />

    <EditText
        android:id="@+id/field_Resultados"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button_ProximoTeste"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginRight="20dp"
        android:layout_toStartOf="@+id/button_ProximoTeste"
        android:layout_toLeftOf="@+id/button_ProximoTeste"
       />


    <Button
        android:id="@+id/button_ProximoTeste"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:text="@string/button_Proximo"
        android:background="@color/colorPrimary"
        android:layout_toRightOf="@+id/text_Resultados"
        android:layout_toEndOf="@+id/text_Resultados"
        android:onClick="onClick_proximoTeste"
        />

</RelativeLayout>
